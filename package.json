{
  "name": "layer0-dojo-example",
  "version": "1.0.0",
  "scripts": {
    "layer0:dev": "layer0 dev",
    "layer0:build": "layer0 build",
    "layer0:deploy": "layer0 deploy",
    "dev": "dojo build --mode dev --watch --serve",
    "build": "dojo build --mode dist",
    "build:dev": "dojo build --mode dev",
    "test": "dojo test",
    "test:unit": "dojo build --mode unit && dojo test --unit --config local",
    "test:functional": "dojo build --mode functional && dojo test --functional --config local",
    "test:all": "dojo build --mode unit && dojo build --mode functional && dojo test --all --config local"
  },
  "dependencies": {
    "@dojo/framework": "~8.0.0",
    "@dojo/widgets": "~8.0.0",
    "tslib": "~1.9.1",
    "cldr-data": "36.0.0",
    "globalize": "1.5.0"
  },
  "devDependencies": {
    "@dojo/cli": "~8.0.0",
    "@dojo/cli-build-app": "~8.0.0",
    "@dojo/cli-test-intern": "~8.0.0",
    "@layer0/cli": "^4.5.0",
    "@layer0/core": "^4.5.0",
    "@layer0/devtools": "^4.5.0",
    "@layer0/prefetch": "^4.5.0",
    "@types/node": "~9.6.5",
    "typescript": "~3.5.3"
  }
}
